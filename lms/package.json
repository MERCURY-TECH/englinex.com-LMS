{
  "name": "LMS",
  "version": "1.0.0",
  "description": "LMS",
  "main": "src/index",
  "types": "src/index",
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "mock"
    ]
  },
  "scripts": {
    "generate-docs": "jsdoc -c ./jsdoc-config.json",
    "build-run": "set NODE_ENV=production && npm run compile && cd build && pm2 start --name LMS index.js -f --watch",
    "buildcp-ui": "cd .. && cd ui && npm run build && cp -r ./dist ../lms",
    "compile": "tsc",
    "start:tsc": "npx ts-node ./src",
    "start:prod": "node ./build/index",
    "start:dev": "nodemon ./src",
    "inspect": "nodemon --inspect src/index.ts",
    "test": "jest --watchAll",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "lint": "standard --verbose | snazzy",
    "node-debug": "DEBUG=express* node src/index.js",
    "chrome-debug": "node --debug-brk --inspect src/index.js"
  },
  "keywords": [
    "express",
    "mongodb",
    "docker",
    "microservice"
  ],
  "author": "Ngum Buka Fon Nyuydze",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.3.4",
    "babel-jest": "^29.5.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^6.0.1",
    "http-status": "^0.2.5",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^2.1.18",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "nodemon": "^2.0.20",
    "pm2": "^5.3.0",
    "socket.io": "^4.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.13",
    "eslint": "^3.13.1",
    "express": "^4.18.2",
    "jsdoc": "^4.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
